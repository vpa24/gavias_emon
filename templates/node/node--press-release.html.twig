{{ attach_library('gavias_emon/press_release') }}

{%
  set classes = [
    'node',
    'node-detail',
    'node--type-' ~ node.bundle|clean_class,
    node.isPromoted() ? 'node--promoted',
    node.isSticky() ? 'node--sticky',
    not node.isPublished() ? 'node--unpublished',
    view_mode ? 'node--view-mode-' ~ view_mode|clean_class,
    'clearfix',
  ]
%}


{% if node.id >= 1489 %}
    {% set pr_contact_name = 'Greg Kaufman' %}
{% else %}
    {% set pr_contact_name = 'Michael Kalodrich' %}
{% endif %}

{% if teaser == true %}

    {% set classes = classes|merge(['news-search-result']) %}
    <article{{attributes.addClass(classes)}}>
        <div class="post-block">
            <div class="post-meta">
                <span class="post-created">PRESS RELEASE</span>
            </div>
            <div class="post-content hover-shadow">
                <div class="row">
                    <div class="col-xs-12">

                        <h3{{title_attributes.addClass('post-title')}}>
                            <a href="{{ url }}" target="smci">{{ label }}</a>
                            <div class="post-meta" style="margin:0">{{ content.field_date.0 }}</div>
                        </h3>

                        <div class="post-description">
                            {{ content.body }}
                        </div>

                        <a href="{{ url }}" target="smci" class="smaller semibold">
                            <div class="readmore">More
                                <i class="fa fa-angle-right"></i>
                            </div>
                        </a>

                    </div>
                </div>
            </div>

        </div>
    </article>

    {# 
      <article{{ attributes.addClass(classes) }}>
        <div class="post-block">
          <div class="post-meta">
            <span class="post-created"> {{ content.field_date }} </span>
