{% else %}

    <style>
        .content-full {
            background-color: white;
        }
    </style>
    <article{{attributes.addClass(classes)}}>
        <div class="post-block">

            <div class="post-thumbnail">

                {% if gva_iframe or content.field_post_gallery|e('html')|trim %}

                    {% if gva_iframe %}
                        <div class="thumbnail-video">
                            {{ gva_iframe|raw }}
                        </div>
                    {% endif %}
                    {% if content.field_post_gallery %}
                        <div class="thumbnail-gallery">
                            {{ content.field_post_gallery }}
                        </div>
                    {% endif %}

                {% else %}

                    <div class="thumbnail-image">
                        {{ content.field_image }}
                    </div>

                {% endif %}

            </div>

            <div class="post-content">
                <h1{{title_attributes.addClass('post-title')}}>{{ label }}</h1>
                <div class="pr-summary">{{ content.body['#object'].body.summary }}</div>

                <div{{content_attributes.addClass('node__content','clearfix')}}>
                    {{ content|without('field_image','field_image_file','field_pr_footer') }}
                </div>

                <div class="pr-image">
                    {% if content.field_image_file['#items'][0].getString() is not empty %}
                        <img src="{{ content.field_image_file['#items'][0].getString() }}">
                    {% elseif content.field_image.entity.fileuri|render is not empty %}
                        <img src="{{ file_url(content.field_image.entity.fileuri) }}">
                    {% endif %}
                </div>

